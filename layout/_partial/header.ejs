<header class="header">
    <span class="header-toggle-all">
            <span class="header-toggle">        
            <a href="<%= url_for() %>" class="logo">
                <img class="logo-img" src="<%= url_for() %>images/logo.png" alt="logo" style="width: 50px; height: 50px;margin: 0px"/>
            </a>
            <a href="<%= url_for() %>" class="logo logo-title hover-underline">
                <h1><%= config.title %></h1>
            </a>
            </span>
    <div></div>
    <nav class="navbar">
        <ul class="menu">
            <% for (let name in theme.menu) { %>
                <% let [url, icon] = theme.menu[name].split('||').map(item => item.trim()); %>
                <li class="menu-item">
                    <a href="<%= url %>" class="menu-item-link">
                        <i class="fa <%= icon %>" aria-hidden="true"></i>
                        <p class="hover-underline"><%= name %><p>
                    </a>
                </li>
            <% } %>
        </ul>
    </nav>
    </span>
</header>
<%- css('css/style') %>
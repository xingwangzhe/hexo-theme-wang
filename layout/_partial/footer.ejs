      <a href="<%= theme.footer_links[i].link %>" alt="<%= theme.footer_links[i].name %>" >
        <div class="link-box">
        <img src="<%= theme.footer_links[i].img %>" class="footer-link-img nofancybox" alt="<%= theme.footer_links[i].name %>" />
